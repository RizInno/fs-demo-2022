{
	"name": "fs-demo-2022",
	"version": "1.0.0",
	"description": "A simple CAP project.",
	"repository": "<Add your repository here>",
	"license": "UNLICENSED",
	"private": true,
	"dependencies": {
		"@sap/cds": "^5",
		"@sap/xb-msg-amqp-v100": "^0.9.58",
		"@sap/xssec": "^3",
		"express": "^4",
		"hdb": "^0.19.0",
		"passport": "^0",
		"ws": "^8.5.0"
	},
	"devDependencies": {
		"@sap/eslint-plugin-cds": "^2.3.5",
		"eslint": "^8.13.0",
		"eslint-html-reporter": "^0.7.4",
		"sqlite3": "^5.0.2"
	},
	"scripts": {
		"start": "cds run",
		"build": "cd app/map && npm install --force && npm run build && cd ../..",
		"lint": "eslint . --max-warnings 0",
		"lint-ci": "eslint . --max-warnings 0 -f node_modules/eslint-html-reporter/reporter.js -o ./report/lint-results.html"
	},
	"cds": {
		"requires": {
			"[production]": {
				"db": {
					"kind": "hana-cloud"
				},
				"auth": {
					"kind": "xsuaa"
				},
				"approuter": {
					"kind": "cloudfoundry"
				},
				"API_EAM_SERVICE": {
					"kind": "odata-v2",
					"model": "srv/external/API_EAM_SERVICE",
					"credentials": {
						"destination": "eam-api-service"
					}
				},
				"API_EHS_REPORT_INCIDENT_SRV": {
					"kind": "odata-v2",
					"model": "srv/external/ehs-edmx",
					"credentials": {
						"destination": "ehs-api-service",
						"path": "/sap/opu/odata/sap/API_EHS_REPORT_INCIDENT_SRV"
					}
				}
			},
			"db": {
				"kind": "hana-cloud"
			},
			"messaging": {
				"kind": "enterprise-messaging-shared",
				"format": "cloudevents",
				"publishPrefix": "$namespace/",
				"subscribePrefix": "$namespace/"
			},
			"API_EAM_SERVICE": {
				"kind": "odata-v2",
				"model": "srv/external/API_EAM_SERVICE",
				"credentials": {
					"url": "http://appsrv16-eam.rizing.io:8000/sap/opu/odata/sap/ZRIZIO_SRV",
					"username": "<username>",
					"password": "<password>"
				}
			},
			"API_EHS_REPORT_INCIDENT_SRV": {
				"kind": "odata-v2",
				"model": "srv/external/ehs-edmx",
				"credentials": {
					"url": "http://appsrv16-eam.rizing.io:8000/sap/opu/odata/sap/API_EHS_REPORT_INCIDENT_SRV",
					"username": "<username>",
					"password": "<password>"
				}
			}
		}
	}
}